<pre tabindex="0" class="chroma"><code class="language-cs" data-lang="cs"><span class="k">static</span> <span class="k">void</span> <span class="n">DuyDang_QueryFilter</span><span class="p">(</span><span class="n">Args</span> <span class="m">_</span><span class="n">args</span><span class="p">)</span>
<span class="p">{</span>
    <span class="n">Query</span> <span class="n">query</span><span class="p">;</span>
    <span class="n">QueryBuildDataSource</span> <span class="n">qbds</span><span class="p">,</span> <span class="n">qbds1</span><span class="p">;</span>
    <span class="n">QueryRun</span> <span class="n">queryRun</span><span class="p">;</span>    
    <span class="n">DuyDang_ParentTable</span> <span class="n">parentTable</span><span class="p">;</span>
    <span class="n">DuyDang_ChildTable</span> <span class="n">childTable</span><span class="p">;</span>
    <span class="n">QueryFilter</span> <span class="n">qFilter</span><span class="p">;</span>
    <span class="n">QueryBuildRange</span> <span class="n">qRange</span><span class="p">;</span>
    <span class="k">struct</span> <span class="nc">structSet</span><span class="p">;</span>

    <span class="n">structSet</span> <span class="p">=</span> <span class="k">new</span> <span class="k">struct</span>
